{
  //   FILES
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "files.simpleDialog.enable": true,
  //   SEARCH
  "search.quickOpen.includeHistory": false,
  "search.exclude": {
    "**/.dart_tool": true,
    "**/.ipynb_checkpoints": true,
    "**/env": true
  },
  //   WORKBENCH
  "workbench.startupEditor": "none",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.preferredLightColorTheme": "Default Light+",
  "workbench.preferredDarkColorTheme": "One Dark Pro Darker",
  "workbench.colorTheme": "One Dark Pro Darker",
  "workbench.editor.enablePreview": false,
  //   EDITOR
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "editor.rulers": [120],
  "editor.renderWhitespace": "all",
  "editor.renderControlCharacters": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.formatOnSave": true,
  "editor.linkedEditing": true,
  "editor.detectIndentation": false,
  "editor.suggest.localityBonus": true,
  "editor.stickyScroll.enabled": true,
  "editor.unicodeHighlight.invisibleCharacters": true,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "editor.unicodeHighlight.includeComments": true,
  //"editor.cursorSurroundingLines": 1000,
  "editor.smoothScrolling": true,
  "editor.experimentalGpuAcceleration": "on",
  //   WINDOW
  "window.openFoldersInNewWindow": "off",
  "window.newWindowDimensions": "maximized",
  "window.restoreFullscreen": true,
  //   COPILOT
  "github.copilot.enable": {
    // you can toggle with alt+\
    "*": false
  },
  //   GIT
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.defaultCloneDirectory": "D:\\git",
  "github.gitProtocol": "ssh",
  //   JIRA & BITBUCKET
  "atlascode.bitbucket.enabled": true,
  "atlascode.jira.enabled": true,
  //   SPELLRIGHT
  "spellright.configurationScope": "user",
  "spellright.language": ["en", "it"],
  "spellright.documentTypes": ["latex", "markdown", "text", "plaintext"],
  "spellright.ignoreFiles": ["**/.gitignore", "**/.spellignore", "**/.vscode/"],
  "spellright.parserByClass": {
    "dart": {
      "parser": "code"
    }
  },
  //   PROJECTMANAGER
  "projectManager.any.baseFolders": ["D:\\git"],
  "projectManager.any.maxDepthRecursion": 1,
  "projectManager.sortList": "Recent",
  "projectManager.any.ignoredFolders": [
    "node_modules",
    ".metadata" //eclipse metada folder
  ],
  //   SETTINGSSYNC
  "settingsSync.ignoredSettings": [
    "yaml.schemas",
    "projectManager.any.baseFolders"
  ],
  "settingsSync.keybindingsPerPlatform": false,
  //   TODOTREE
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  //   PHONE EMULATOR
  "emulator.androidColdBoot": true,
  "emulator.emulatorPathWindows": "D:\\androidSDK\\emulator",
  //   ZEN MODE
  "zenMode.hideLineNumbers": false,
  "zenMode.fullScreen": false,
  //   ERROR LENS
  "errorLens.enabledDiagnosticLevels": ["warning", "info", "error", "hint"],
  "errorLens.enabled": false,
  //   LANGUAGE-SPECIFIC SETTINGS
  //DART
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code"
  },
  "dart.lineLength": 120,
  "dart.warnWhenEditingFilesOutsideWorkspace": false,
  //PHP
  "php.validate.executablePath": "c:/xampp/php/php.exe",
  //JAVA
  //YAML
  "yaml.schemas": {
    "file:///home/mattia/.vscode-server/extensions/atlassian.atlascode-3.8.6/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  //PYTHON
  //MARKDOWN
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.rulers": []
  },
  "markdown.updateLinksOnFileMove.enabled": "always",
  //JSON
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  //XML
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  //YAML
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  //CSS
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  //   TERMINAL
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "telemetry.telemetryLevel": "off",
  "redhat.telemetry.enabled": false,
  "color-highlight.markerType": "dot-before",
  "explorer.confirmDelete": false,
  "testExplorer.useNativeTesting": true,
  "security.workspace.trust.enabled": false,
  "hediet.vscode-drawio.resizeImages": null,
  "dev.containers.executeInWSL": true
}

{
  "from": "none",
  "shell": "bash",
  "paths":{
    "scripts/*": "~/.local/bin/",
    "etc/*": {
      "path": "/etc/",
      "owner": "root"
    }
    "bash/bashrc": {
      "path": "~/.bashrc",
      "strategy": "concatenate"
    }
    "bash/bash_profile": {
      "path": "~/.bash_profile",
      "strategy": "concatenate"
    }
    "hushlogin": "~/.hushlogin"
  }
}
